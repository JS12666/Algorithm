    "def Hanoi(n , fro, by, to):\n",
    "    if(n==1):\n",
    "        print(f\"Move {fro} -> {to}\")\n",
    "    else:\n",
    "        Hanoi(n - 1, fro, to, by)\n",
    "        print(f\"Move {fro} -> {to}\")\n",
    "        Hanoi(n - 1, by, fro, to)"
   ]
